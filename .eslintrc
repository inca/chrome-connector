{
  "extends": "ub",
  "env": {
    "es6": true,
    "node": true,
    "browser": false
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "script"
  },
  "rules": {
    "no-console": 2,
    "init-declarations": [2, "always"],
    "no-use-before-define": 0,
    "prefer-arrow-callback": 0,
    "max-nested-callbacks": 0,
    "no-eval": 2
  }
}
